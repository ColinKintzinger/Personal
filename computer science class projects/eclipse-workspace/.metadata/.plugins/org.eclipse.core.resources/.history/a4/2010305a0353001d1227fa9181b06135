import java.util.Scanner;

public class part1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("enter a value:");
		int a = sc.nextInt();
		for (int i= a; 1<= i; i--) {
			for(int c= i; 1<=c; c--)
			System.out.print(i);
		}
		
		

	}// end of main
}// end of class
